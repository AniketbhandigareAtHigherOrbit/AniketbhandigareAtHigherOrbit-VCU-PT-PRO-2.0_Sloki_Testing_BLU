				<RegexPatternGroup TokenId="45" TokenKey="Label" Pattern="([\._@\$\?] | {Alpha})([@\$\?] | {Word})* \:" CaseSensitivity="Insensitive" />
				<RegexPatternGroup TokenId="11" TokenKey="Identifier">
					<RegexPattern Value="([\._@\$\?] | {AlphaMacro})([@\$\?\.] | {WordMacro})*" />
					<RegexPattern Value="{Digit}+([@\$\?\.] | {WordMacro})*" />
				</RegexPatternGroup>
				<RegexPatternGroup TokenId="13" TokenKey="Number" ClassificationTypeKey="Number" LookAheadPattern="{NonWord}|\z">
					<RegexPattern Value="0 [Xx] {HexDigitMacro}+" />
					<RegexPattern Value="0 [0-7]*" />
					<RegexPattern Value="0 [Bb] [0-1]+" />
					<RegexPattern Value="[1-9][0-9]*" />
