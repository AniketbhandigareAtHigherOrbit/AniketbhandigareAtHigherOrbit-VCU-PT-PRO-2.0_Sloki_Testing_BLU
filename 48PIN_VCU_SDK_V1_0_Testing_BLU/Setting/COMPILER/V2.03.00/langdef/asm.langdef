				</ExplicitPatternGroup>
				<ExplicitPatternGroup TokenId="36" TokenKey="Hardware_Instruction" ClassificationTypeKey="ReservedWord" LookAheadPattern="{NonWord}|\z" CaseSensitivity="Insensitive">
