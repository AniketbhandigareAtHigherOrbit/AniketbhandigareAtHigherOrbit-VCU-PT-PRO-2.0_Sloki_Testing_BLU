			<!-- Character state -->
			<State Id="2" Key="Character" DefaultTokenId="16" DefaultTokenKey="CharacterDefault" DefaultClassificationTypeKey="Default">
				<State.Scopes>
					<Scope>
						<Scope.StartPatternGroup>
							<ExplicitPatternGroup TokenId="17" TokenKey="CharacterStart" Pattern="'" />
						</Scope.StartPatternGroup>
						<Scope.EndPatternGroup>
							<RegexPatternGroup TokenId="18" TokenKey="CharacterEnd" Pattern="['\n]" />
						</Scope.EndPatternGroup>
					</Scope>
				</State.Scopes>
				<RegexPatternGroup TokenId="19" TokenKey="CharacterEscapedCharacter" Pattern="\\." />
				<RegexPatternGroup TokenId="20" TokenKey="CharacterWhitespace" Pattern="{Whitespace}+" />
				<RegexPatternGroup TokenId="16" TokenKey="CharacterDefault" Pattern="[^'\n]" />
			</State>
			<!-- String state -->
			<State Id="3" Key="String" DefaultTokenId="21" DefaultTokenKey="StringDefault" DefaultClassificationTypeKey="String">
				<State.Scopes>
					<Scope>
						<Scope.StartPatternGroup>
							<ExplicitPatternGroup TokenId="22" TokenKey="StringStart" Pattern="&quot;" />
						</Scope.StartPatternGroup>
						<Scope.EndPatternGroup>
							<RegexPatternGroup TokenId="23" TokenKey="StringEnd" Pattern="[\&quot;\n]" />
						</Scope.EndPatternGroup>
					</Scope>
				</State.Scopes>
				<RegexPatternGroup TokenId="24" TokenKey="StringEscapedCharacter" Pattern="\\." />
				<RegexPatternGroup TokenId="25" TokenKey="StringWhitespace" Pattern="{Whitespace}+" />
				<RegexPatternGroup TokenId="21" TokenKey="StringDefault" Pattern="[^\&quot;\n]" />
			</State>
			<!-- Comment state -->
			<State Id="5" Key="Comment" DefaultTokenId="32" DefaultTokenKey="SingleLineCommentText" DefaultClassificationTypeKey="Comment">
				<State.Scopes>
					<Scope>
						<Scope.StartPatternGroup>
							<ExplicitPatternGroup Pattern=";" />
						</Scope.StartPatternGroup>
						<Scope.EndPatternGroup>
							<RegexPatternGroup Pattern="\n" />
						</Scope.EndPatternGroup>
					</Scope>
					<Scope>
						<Scope.StartPatternGroup>
							<RegexPatternGroup Pattern="^ {Whitespace}* #" />
						</Scope.StartPatternGroup>
						<Scope.EndPatternGroup>
							<RegexPatternGroup Pattern="\n" />
						</Scope.EndPatternGroup>
					</Scope>
				</State.Scopes>
				<RegexPatternGroup TokenId="32" TokenKey="CommentWhitespace" Pattern="{Whitespace}+" />
				<RegexPatternGroup TokenId="46" TokenKey="CommentWord" Pattern="\w+" />
				<RegexPatternGroup TokenId="47" TokenKey="CommentDefault" Pattern="{NonLineTerminator}+" />
			</State>
